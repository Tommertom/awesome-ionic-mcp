{
  "name": "awesome-ionic-mcp",
  "version": "0.1.23",
  "description": "A Model Context Protocol server for Ionic and Capacitor",
  "type": "module",
  "bin": {
    "awesome-ionic-mcp": "./bin/mcp-server.js"
  },
  "files": [
    "build",
    "bin"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "watch": "tsc --watch",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js",
    "start": "node build/index.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.1",
    "execa": "^9.6.0",
    "find-up": "^8.0.0",
    "js-yaml": "^4.1.0",
    "puppeteer": "^24.9.0",
    "strip-ansi": "^7.1.2",
    "tree-kill": "^1.2.2"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.24",
    "typescript": "^5.3.3"
  }
}
